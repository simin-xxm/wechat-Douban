/* pages/index/seemore/seemore.wxss */
swiper{
    margin-top: 50rpx;
    height: 550rpx;
    background-color: saddlebrown;
  }
   
  .slide-image {
    width: 100%;
    height: 100%;
    background-color: rosybrown;
  }
   
  .zoom-out {
    transform: scale(0.8);
    transition: all 0.7s ease-out 0s;
  }
   
  .zoom-in {
    transform: scale(1);
    transition: all 0.7s ease-in 0s;
  }
